<div id="calendar"></div>

<script type="text/javascript">
$(document).ready(function () {
	
	$('#calendar').fullCalendar({
		events: <%= raw(@events) %>,

		eventClick: function(event) {
			if (event.url) {
				window.open(event.url);
				return false;
			}
		},
		eventColor: '#FF4209'
	});
});
</script>